<div class="group-input-fields-content" data-callback-render="custom_field_checkboxs_val">
    <% field.options[:multiple_options].each_with_index do |option, index|  %>
        <div class="checkbox"><label><input type="checkbox" value="<%= option[:value] %>" name="<%= field_name %>[<%= field.slug %>][values][<%= index %>]" <%= 'checked' if default_use && option[:default].present? %> > <%= option[:title] %></label></div>
    <% end %>
</div>
